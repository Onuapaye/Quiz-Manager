package fr.fortress.quizmanager.model;

import javax.persistence.Entity;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

@Entity
@Table(name ="tblMCQAnswer", schema = "fortressqmschema")
public class MCQAnswer {
	
	int studentId;
	int examId;
	int choiceId;
	
	@ManyToOne
	private MCQChoice mcqChoice;
	
	@ManyToOne
	private Student student;
	
	@ManyToOne
	private Exam exam;
	
	
	
	public MCQChoice getMcqChoice() {
		return mcqChoice;
	}
	public void setMcqChoice(MCQChoice mcqChoice) {
		this.mcqChoice = mcqChoice;
	}
	public Student getStudent() {
		return student;
	}
	public void setStudent(Student student) {
		this.student = student;
	}
	public Exam getExam() {
		return exam;
	}
	public void setExam(Exam exam) {
		this.exam = exam;
	}
	public int getStudentId() {
		return studentId;
	}
	public void setStudentId(int studentId) {
		this.studentId = studentId;
	}
	public int getExamId() {
		return examId;
	}
	public void setExamId(int examId) {
		this.examId = examId;
	}
	public int getChoiceId() {
		return choiceId;
	}
	public void setChoiceId(int choiceId) {
		this.choiceId = choiceId;
	}

	
}
